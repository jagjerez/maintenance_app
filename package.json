{
  "name": "maintenance_app",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:with-cron": "concurrently \"next dev --turbopack\" \"node scripts/start-local-cron.js\"",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "init-db": "node scripts/init-db.js",
    "update-locales": "node scripts/update-locales.js",
    "add-language": "node scripts/add-language.js",
    "cron:start": "node scripts/start-local-cron.js",
    "cron:status": "curl http://localhost:3000/api/cron/local",
    "cron:process": "curl -X POST http://localhost:3000/api/cron/local -d '{\"action\":\"process\"}'",
    "update-templates": "node scripts/update-templates.js"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.10.0",
    "@hookform/resolvers": "^5.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/mongoose": "^5.11.96",
    "@vercel/blob": "^1.1.1",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "csv-parser": "^3.2.0",
    "lucide-react": "^0.544.0",
    "mongoose": "^8.18.1",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "next-intl": "^4.3.8",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
